@model IEnumerable<SchibstedBackendTest.Models.UserViewModel>

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.username)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsADMIN)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsPAGE_1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsPAGE_2)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsPAGE_3)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.username)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsADMIN)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsPAGE_1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsPAGE_2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsPAGE_3)
            </td>
        </tr>
    }
</table>